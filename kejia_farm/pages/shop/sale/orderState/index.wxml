<view><view class='block20'></view><view><view class='line'><view class='lines'><image class='lineImg' src='../../../../images/icon/address-line.png'></image></view></view><view class='p20'><view class='flexSb  f32'><view class='flex'><text class='iconfont icon-yonghu f44' style='color:{{farmSetData.background_color}}'></text><view class='ml10'>{{orderData.name}}</view></view><view class='flex'><text class='iconfont icon-phone f44' style='color:{{farmSetData.background_color}}'></text><view class='ml10'>{{orderData.phone}}</view></view></view><view class='f30 cf7e7e7e mt20'>{{orderData.address}}</view></view><view class='line mt10'><view class='lines'><image class='lineImg' src='../../../../images/icon/address-line.png'></image></view></view></view><view class='block20'></view><view class='bgff p20'><block wx:for='{{orderData.orderDetail}}' wx:for-item='item' wx:for-index='index' wx:key='{{index}}'><view class='flex ptb20 bb'><image class='headImg' src='{{item.cover}}'></image><view class='ml20'><view class='itemName'>{{item.goods_name}}</view><view class='f26 mt10 cf93'><block wx:for='{{item.specItem}}' wx:for-item='val' wx:for-index='index' wx:key='{{index}}'><text>{{val.name}}:</text><text class='ml10'>{{val.spec_value}};</text></block><text class='ml10'>X{{item.count}}</text></view><view><block wx:if='{{type != 3}}'><text class='f26'>￥{{item.price}}</text></block><block wx:else><text class='f26'>{{item.price}}积分</text></block></view></view></view></block></view><view class='block20'></view><view class='flex ptl30 f30 bb'><view>买家留言:</view><block wx:if='{{orderData.remark}}'><view class='ml10'>{{orderData.remark}}</view></block><block wx:else><view class='ml10'>无</view></block></view><view class='ptl30 f30 bb'><view class='flexSb  f34'><view>支付状态</view><block wx:if='{{orderData.status == 0}}'><view class='fb cfred'>未支付</view></block><block wx:if='{{orderData.status == 1}}'><view class='fb cfred'>待发货</view></block><block wx:if='{{orderData.status == 2}}'><view class='fb cfred'>已发货</view></block><block wx:if='{{orderData.status == 3}}'><view class='fb cfred'>已完成</view></block><block wx:if='{{orderData.status == 4}}'><view class='fb cfred'>申请取消</view></block><block wx:if='{{orderData.status == 5}}'><view class='fb cfred'>已取消</view></block></view><view class='flex mt20'><view class='cf93'>支付方式：</view><view class='ml10'>{{orderData.pay_method}}</view></view><view class='flex mt20'><view class='cf93'>下单时间：</view><view class='ml10'>{{orderData.create_time}}</view></view><view class='flex mt20'><view class='cf93'>支付时间：</view><view class='ml10'>{{orderData.pay_time}}</view></view></view><view class='ptl30 f30 bb'><view class='flexSb  f34'><view>配送信息</view><block wx:if='{{ (orderData.status == 0 || orderData.status == 1) }}'><view class='fb cfred'>未配送</view></block><block wx:if='{{orderData.status == 2}}'><view class='fb cfred'>已配送</view></block></view><view class='flex mt20'><view class='cf93'>快递公司：</view><view class='ml10'>{{orderData.express_company}}</view></view><view class='flex mt20'><view class='cf93'>快递单号：</view><view class='ml10'>{{orderData.send_number}}</view></view></view><view class='ptl30 f30 bb'><view class='flexSb  f34'><view>合计：</view><block wx:if='{{type != 3}}'><view class='fb cfred'>￥{{orderData.total_price}}</view></block><block wx:else><view class='fb cfred'>{{orderData.total_price}}积分</view></block></view><view class='flexRight mt20'><view>+运费</view><view class='w300'>￥{{orderData.send_price}}</view></view></view><view class='flexRight'><block wx:if='{{ (orderData.status == 0 || orderData.status == 1) }}'><button bindtap='cancelOrder' class='cancelOrder' data-orderid='{{orderData.id}}' style='color:{{farmSetData.background_color}};border:1rpx solid {{farmSetData.background_color}}'>取消订单</button></block><block wx:if='{{orderData.status == 1}}'><button bindtap='fahuo' class='confirm' style='background:{{farmSetData.background_color}}'>立即发货</button></block></view><view catchtouchmove='preventTouchMove' class='model {{(isShow?"opacity0":"")}}'><form bindsubmit='sendOrder'><view class='info_container {{(isShow?"bottom0":"bottom1")}}'><view class='model-title' style='color:{{farmSetData.background_color}}'>快递发货</view><picker bindchange='bindPickerChange' range='{{express}}' value='{{index}}'><view class='info_item bb'><view class='Station'></view><view class='express_info'><input name='send_number' placeholder='快递公司' value='{{expressName}}'></input></view><view class='Station'><text class='iconfont icon-down f44'></text></view></view></picker><view class='info_item'><view class='Station'></view><view class='express_info'><input name='send_number' placeholder='快递单号' value='{{send_number}}'></input></view><view class='Station'><text bindtap='scanCode' class='iconfont icon-saoma f44'></text></view></view><view class='flex'><button bindtap='closeModel' class='cancelBtn'>取消</button><button class='confrimBtn' formType='submit' style='background:{{farmSetData.background_color}}'>确定</button></view></view></form></view></view>